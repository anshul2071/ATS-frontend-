{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",

    "baseUrl": "./",
    "paths": {
      "@fullcalendar/list": ["node_modules/@fullcalendar/list"]
    },

    

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*", "src/lib/**/*"],
}
